{
  "name": "payjp",
  "version": "1.3.0",
  "description": "PAY.JP node.js bindings",
  "main": "built/index.js",
  "types": "built/index.d.ts",
  "keywords": [
    "PAY.JP",
    "payment processing",
    "credit cards",
    "api"
  ],
  "scripts": {
    "clean": "rm -rf ./.nyc_output ./node_modules/.cache ./coverage",
    "test": "npm run lint && npm run build && nyc mocha",
    "build": "tsc",
    "lint": "eslint . -c .eslintrc.json --ignore-path .gitignore --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "report": "nyc -r text -r lcov report",
    "prepublish": "npm run build",
    "coverage": "codecov -f coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/payjp/payjp-node.git"
  },
  "author": "PAY.JP <support@pay.jp> (https://pay.jp)",
  "contributors": [
    "Yoichi Fujimoto <wozozo@gmail.com>",
    "Daiki Arai <darai0512@yahoo.co.jp>"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "@types/superagent": "^3.8.0",
    "@typescript-eslint/eslint-plugin": "^1.1.0",
    "@typescript-eslint/parser": "^1.1.0",
    "codecov": "^3.2.0",
    "nyc": "^13.3.0",
    "eslint": "^5.12.0",
    "mocha": "^6.0.2",
    "typescript": "^3.3"
  },
  "dependencies": {
    "superagent": "^3.8.3"
  }
}
